<h1>New product</h1>

<% form_for @product, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :Tag %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', products_path %><br/><br/>
<%= link_to 'LOGOUT', logout_path %>